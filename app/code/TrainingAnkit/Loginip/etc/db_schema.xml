<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="trainingankit_loginip" resource="default" engine="innodb" comment="Crud Form">
        <column xsi:type="int" name="customer_id" padding="10" unsigned="true" nullable="false" identity="true" comment="customer ID"/>
        <column xsi:type="varchar" length="255" name="name" nullable="false" comment="Name"/>
        <column xsi:type="varchar" length="255" name="address" nullable="false" comment="Ip Address"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="customer_id"/>
        </constraint>
    </table>
</schema>